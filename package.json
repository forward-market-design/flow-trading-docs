{
	"name": "flow-trading-docs",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro build",
		"preview": "astro preview",
		"astro": "astro"
	},
	"dependencies": {
		"@astrojs/starlight": "^0.34.2",
		"astro": "^5.7.10",
		"sharp": "^0.34.1",
		"starlight-openapi": "^0.16.0"
	},
	"devDependencies": {
		"prettier": "^3.5.3",
		"prettier-plugin-astro": "^0.14.1"
	},
	"prettier": {
		"trailingComma": "all",
		"tabWidth": 2,
		"useTabs": true,
		"singleQuote": false,
		"printWidth": 120,
		"plugins": [
			"prettier-plugin-tailwindcss",
			"prettier-plugin-astro"
		],
		"overrides": [
			{
				"files": "*.astro",
				"options": {
					"parser": "astro"
				}
			}
		]
	}
}
